.route, .track {
  ::glow1 {
    ::glow2 {
      line-width: 20;
      line-opacity: .15;
      line-color: green;
    }
    line-width: 6;
    line-opacity: .5;
    line-color: yellow;
  }
  line-width: 1;
  line-opacity: .8;
  line-color: red;
}

.points {
  marker-width:6;
  marker-fill:#f45;
  marker-line-color:#813;
  marker-allow-overlap:true;
 }
